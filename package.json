{
  "name": "@your-scope/component-map",
  "version": "1.0.0",
  "description": "TypeScript implementation of Spring Boot's ComponentMap pattern for strategy pattern and dependency injection",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "demo": "npm run build && node dist/demo.js",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "start": "npm run demo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "componentmap",
    "strategy-pattern",
    "dependency-injection",
    "typescript",
    "spring-boot-pattern",
    "design-patterns",
    "plugin-architecture",
    "event-handlers"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/ts-component-map.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/ts-component-map/issues"
  },
  "homepage": "https://github.com/your-username/ts-component-map#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13 || ^0.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
